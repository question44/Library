  <children>
    <TextField fx:id="txtSearch" layoutX="135.0" layoutY="178.0" prefHeight="33.0" prefWidth="330.0" promptText="Wpisz szukaną frazę..." text="" />
    <Button fx:id="btnSearch" layoutX="437.0" layoutY="222.0" mnemonicParsing="false" onAction="#btnSearchAction" text="Szukaj" />
    <Button fx:id="btnCatalog" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnCatalogAction" prefHeight="21.0" prefWidth="95.0" text="Katalog" />
